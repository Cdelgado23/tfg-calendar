{
  "name": "tfg-calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "bootstrap": "^4.6.0",
    "firebase": "^8.3.1",
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.0",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-loading-overlay": "^1.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-spinners": "^0.10.6",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/repository/*.js",
      "!src/*.js"
    ]
  },
  "homepage": ".",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "sh -ac '. ./.env.${REACT_APP_ENVIROMENT}; react-scripts start'",
    "start:local": "REACT_APP_ENVIROMENT=local npm run start",
    "start:dev": "REACT_APP_ENVIROMENT=dev npm run start",
    "build": "sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:pro": "REACT_APP_ENV=pro npm run build",
    "test": " react-scripts test",
    "testWithEnv": "sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts test --coverage'",
    "test:local": "REACT_APP_ENV=local npm run testWithEnv",
    "cypress:open": "cypress open",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^7.6.0"
  }
}
